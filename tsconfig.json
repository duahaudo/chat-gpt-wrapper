{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es6",
    /* Modules */
    "module": "es2022",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  }
}
